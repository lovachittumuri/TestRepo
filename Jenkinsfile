    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello I am the sexy Fellow'
            }
        } 
        stage('Hi') {
            steps {
                echo 'Hi I am from Git Repo'
            }
        }
        stage('Hey') {
            steps {
                echo 'Hey this is Git Case'
            }
        }
    }
}
